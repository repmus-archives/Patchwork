(IN-PACKAGE :PW)(MAKE-WIN 'C-PW-WINDOW "chord notes" 2555951 14090670 (LIST (SBOX 'C-PW-TEXT-BOX:C-PW-TEXT-BOX 'C-PW-TEXT-BOX::TEXT "text" NIL 5308510 (LIST "Evaluer ici") 2031702) (SBOX 'C-AAC 'AAC "aac" NIL 3801204 NIL NIL '("tell application \"pw2.6.3.image\"lock box \"chord\"command menu \"Chord Object\" of box \"chord\"set mychord to (eval box \"chord\")make new patchrepeat with i from 1 to 5set thenote to note i of mychordset mybox to make box of class \"mk-note\" at { ((i - 1) * 100), 50 }set input 1 of mybox to (pitch of thenote as integer) set input 2 of mybox to (duration of thenote as integer)set input 3 of mybox to (velocity of thenote as integer)set input 4 of mybox to (time of thenote as integer)end repeatend tell" NIL)) (SBOX 'C-PW-TEXT-BOX:C-PW-TEXT-BOX 'C-PW-TEXT-BOX::TEXT "text" NIL 524495 (LIST " Le script contenu dans aac analyse l'accord dans la boite chord et construit un nouveau patch contenant une boite \"mk-note\" pour chacune des notes de l'accord.") 5505225) (SBOX 'C-PATCH-CHORD-BOX-M 'CHORD "chord" NIL 589865 (LIST (NOTE (LIST 5900 20 0 1 NIL 1 100 NIL) (LIST 6000 40 0 2 NIL 1 80 NIL) (LIST 6400 60 0 3 NIL 1 60 NIL) (LIST 6700 80 0 4 NIL 1 40 NIL) (LIST 7100 100 0 5 NIL 1 20 NIL))) NIL :OBJECT)) (LIST) T)