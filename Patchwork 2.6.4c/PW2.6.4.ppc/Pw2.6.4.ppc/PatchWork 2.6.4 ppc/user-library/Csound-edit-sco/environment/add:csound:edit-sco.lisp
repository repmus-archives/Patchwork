(in-package "CS-E")(ccl::export '(inharm-ser))(PW::defunp inharm-ser ((begin fix/float)                    (dist fix/float)                    (npart fix/float)) list        "produit une liste de npart partiels ˆ partir de la valeur beginselon la formule partiel = begin * rang^dist"        (let ((L ()))          (dotimes (n npart (reverse L))            (push (* begin (expt (1+ n) dist)) L))))