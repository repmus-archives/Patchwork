                                   ;(ORCHESTRE)add-cplet1                                                   ; SONS SINUSOIDAUX				 ;fréquence                      amplitude;1/ + UNE ENVELOPPE             1/ + UNE ENVELOPPE  ;2/ + UN GLISSANDO              2/ + UN VIBRATO     (+env.);3/ + UN VIBRATO (+env.)        3/ + UN JITTER      (+env.);4/ + UN JITTER  (+env.);p1 =ins;p2 =date;p3 = dur;p4 = amp globale;p5 = n° table freq globale;p6 = n° table d'env d'amplitude;frequence;p7 = n° table freq vibra  ;valeur 7.5 = vibrato;p8 = n° table env vibra   ;valeur 1 = vibrato valeur 100 = FM forte;p9 = n° table freq du jitter;p10 = n° table env du jitter;amplitude;p11 = n° table freq vibra;p12 = n° table env vibra;---------------------------------------------------------------------------------------------   sr = 32000   kr = 160   ksmps = 200   nchnls = 1;---------------------------------------------------------------------------------------------     instr 1 	 	                             ;FRÉQUENCE DE L'OSCILLATEUR1 	                                      kgliss1	   oscil1i    0,1,p3,p5  kvitvib1	   oscil1i    0,1,p3,p7                kprofvib1         oscil1i    0,1,p3,p8                kvib1             oscili     kprofvib1,kvitvib1,98      kfreqjitter1	   oscil1i    0,1,p3,p9                    ;définition de la "fréquence du jitter" kampjitter1       oscil1i    0,1,p3,p10                    ;définition de l'amplitude du jitter ajit1             randi     kampjitter1,kfreqjitter1       ;définition d'un jitter                                                                                                 ;------------------------------------						   					;AMPLITUDE DE L'OSCILLATEUR1						    kenvamp           oscil1i    0,p4,p3,p6    kvitvib2          oscil1i    0,1,p3,p11                kprofvib2         oscil1i    0,1,p3,p12                kvib2             oscili     kprofvib2,kvitvib2,1                             ason              oscili  (kenvamp+kvib2),(kgliss1+kvib1+ajit1),1      out  ason	 endin	 	 