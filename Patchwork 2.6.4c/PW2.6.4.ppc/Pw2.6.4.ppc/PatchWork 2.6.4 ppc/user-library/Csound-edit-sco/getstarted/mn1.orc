; Synthétiseur <mn1> 07-93; 		sr = 44100		kr = 900		ksmps = 49		nchnls = 1		instr 1		kalea	randi	p32, p32*5	if p33=0 goto else1		kfreq	linseg	p35, p36, p35, (p3-p36)/p34, p5, p3-((p3-p36)/p34), p5		goto cont1	else1:	kfreq 	= 		p5cont1:	kfreq2	=		(p30 = 0 ? p31 : p7*kfreq)klfo	oscili	p29,kfreq2,p10					; LFOkmodh	=		(p27 = 1 ? klfo : 0)kmodf	=		(p28 = 1 ? (klfo+1)/2 : 1)	kf		=		kfreq + kalea + kmodh*kfreq	aosc1	oscili	p11, kf, p8				; DCOaosc2	oscili	p12, (kf*p6), p9aosc3	oscili	p13, (kf*p7), p10ablanc	rand	p14amix	=		aosc1 + aosc2 + aosc3 + ablanckenv	expseg	0.001, p23, 1, p24, p25, p3-p23-p24-p26, p25, p26, 0.001	; EGkfenv	expseg	0.001, p19, 1, p20, p21, p3-p19-p20-p22, p21, p22, 0.001kfenv	=		kfenv*kmodf*p17kfrfil 	= 		(p18 = 0 ? p15 : p15+kfreq)apb		tone	amix,kfrfil					; DCFafil	reson	apb,kfrfil,p16afil	balance	afil, amixafil2	=		afil*kfenv					; EG Famix2 	= 		amix*(1 - kfenv)		asig  	=		(amix2 + afil2)*kenv*p4		; DCA	out asig	 endin	 	 		 	