; Synthétiseur <mn5c>; additif simple enveloppes par tables		sr = 44100		kr = 900		ksmps = 49		nchnls = 1; p4 = amp tab; p5 = freq tab; p6 = wave tab; p7 = amp factor; p8 = freq factor		instr 1				kenv	oscil1i	0, p7, p3, p4	; enveloppe d'amplitude				khz	oscil1i	0, p8, p3, p5	; enveloppe des hauteurs				ason	oscili	kenv, khz, p6	; oscillateur						out		ason								endin