; instrument <mn6>; additif par notes; p4 = amp (0 - 1); p5 = freq (Hz); p6 = wave n¡; p7 = jitter (0 1 x); p8 = dur attack (sec); p9 = dur release (sec);----------------------------------------------------------------------------------		sr = 44100		kr = 4410		ksmps = 10		nchnls = 1		;----------------------------------------------------------------------------------		instr 1				kenv		linen	p4, p8, p3, p9		; enveloppe standard (A S R)				kjitter1	randi	1,9			; valeurs		kjitter2	randi	0.5,1.3			; pour un jitter		kjitter3	randi	1.2,1.1			; moyen		kjitter		=	kjitter1+kjitter2+kjitter3					ason		oscili	kenv, p5*(1+kjitter*p7), p6	;oscillateur						out		ason*3767								endin